<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0">

  <xs:element name="ptps" type="ptps">
      <xs:annotation>
         <xs:appinfo>
            <space-appinfo>
               <collection/>
            </space-appinfo>
         </xs:appinfo>
      </xs:annotation>
   </xs:element>

  <xs:complexType name="gen_ptps-ptps">
    <xs:sequence>
      <xs:element name="mtu" type="xs:int"/>
      <xs:element name="speedStr" type="xs:string"/>
      <xs:element name="managedDeviceId" type="xs:int"/>
      <xs:element name="speed" type="xs:int"/>
      <xs:element name="tagging" type="xs:string"/>
      <xs:element name="adminStatus" type="xs:string"/>
      <xs:element name="operationStatus" type="xs:string"/>
      <xs:element name="deviceId" type="xs:int"/>
      <xs:element name="linkType" type="xs:string"/>
      <xs:element name="macAddress" type="xs:string"/>
      <xs:element name="id" type="xs:int"/>
      <xs:element minOccurs="0" name="ptpid" type="xs:int">
            <xs:annotation>
               <xs:appinfo>
                  <space-appinfo>
                     <read-only/>
                  </space-appinfo>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
      <xs:element name="loopbackEnabled" type="xs:string"/>
      <xs:element name="managed-dev-ip-address" type="xs:string"/>
      <xs:element name="link-level-type" type="xs:string"/>
      <xs:element name="description" type="xs:string"/>
      <xs:element name="name" type="xs:string"/>
      <xs:element name="mtuStr" type="xs:string"/>
      <xs:element name="info-url" type="xs:string"/>
      <xs:element name="encapsulation" type="xs:string"/>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:string"/>
    <xs:attribute name="href" type="xs:string">
         <xs:annotation>
            <xs:appinfo>
               <space-appinfo>
                  <link>/api/jssdk/swag-dtu/ptps/{pTPId}</link>
               </space-appinfo>
            </xs:appinfo>
         </xs:annotation>
      </xs:attribute>
  </xs:complexType>

  <xs:complexType name="ptps">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="ptp" type="gen_ptps-ptps"/>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:string"/>
    <xs:attribute name="total" type="xs:int"/>
  </xs:complexType>
</xs:schema>
